<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_vip_member_details" model="ir.ui.view">
            <field name="name">会员信息详情</field>
            <field name="model">vip.member.details</field>
            <field name="arch" type="xml">
                <form string="会员信息详情" version="7.0">
                    <group col="4" string="日期">
                        <field name="date_start"/>
                        <field name="date_end"/>
                    </group>
                    <field name="user_ids"/>
                    <footer>
                        <button name="print_report" type="object" string="Print Report" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="action_report_vip_memeber_details" model="ir.actions.act_window">
            <field name="name">会员信息详情</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vip.member.details</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</openerp>
